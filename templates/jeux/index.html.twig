{% extends 'base.html.twig' %}

{% block title %}Jeux index{% endblock %}

{% block body %}
    <h1>Jeux index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Titre</th>
                <th>Type</th>
                <th>AnneeSortie</th>
                <th>Support</th>
                <th>Description</th>
                <th>Editeur</th>
                <th>Developpeur</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for jeux in jeuxes %}
            <tr>
                <td>{{ jeux.titre }}</td>
                <td>{{ jeux.type }}</td>
                <td>{{ jeux.anneeSortie}}</td>
                <td>{{ jeux.support }}</td>
                <td>{{ jeux.description }}</td>
                <td>{{ jeux.editeur }}</td>
                <td>{{ jeux.developpeur }}</td>
                <td>
                    <a href="{{ path('jeux_show', {'id': jeux.id}) }}">show</a>
                    <a href="{{ path('jeux_edit', {'id': jeux.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('jeux_new') }}">Create new</a>
{% endblock %}
